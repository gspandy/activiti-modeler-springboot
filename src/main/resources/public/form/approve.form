<div class="form-group">
	<label for="name" class="col-sm-4 control-label">开始时间:</label>
	<div class="col-sm-5">
		<input type="text" value="${startDate}" disabled class="form-control dateFormat" id="startDate"
			name="startDate">
	</div>
</div>
<div class="form-group">
	<label for="name" class="col-sm-4 control-label ">结束时间:</label>
	<div class="col-sm-5">
		<input type="text" value="${endDate}" disabled class="form-control dateFormat" id="endDate"
			name="endDate">
	</div>
</div>
<div class="form-group">
	<label for="name" class="col-sm-4 control-label">原因</label>
	<div class="col-sm-5">
		<input type="text" value="${reason}"  disabled class="form-control" id="reason"
			name="reason">
	</div>
</div>

<div class="form-group">
	<label for="name" class="col-sm-4 control-label">审批意见</label>
	<div class="col-sm-5">
		<select class="form-control " name="fp_approve" id="approve">
			<option value="true">同意</option>
			<option value="false">拒绝</option>
		</select>
	</div>
</div>